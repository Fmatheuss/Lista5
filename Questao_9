#include <stdio.h>

void bubbleSort(int *vetor, int tamanho) {
    int i, j, temp;
    // Percorre o vetor
    for (i = 0; i < tamanho - 1; i++) {
        for (j = 0; j < tamanho - i - 1; j++) {
            // Compara os elementos adjacentes
            if (*(vetor + j) > *(vetor + j + 1)) {
                // Troca os elementos usando um tempor√°rio
                temp = *(vetor + j);
                *(vetor + j) = *(vetor + j + 1);
                *(vetor + j + 1) = temp;
            }
        }
    }
}

int main() {
    int vetor[5], i;

    // Entrada de dados
    printf("Digite 5 numeros para ordenar:\n");
    for (i = 0; i < 5; i++) {
        printf("Elemento %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    // Ordena o vetor
    bubbleSort(vetor, 5);

    // Exibe o vetor ordenado
    printf("\nVetor ordenado: ");
    for (i = 0; i < 5; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}
